<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebProjekt_Beispiel</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>

    <div id="container">
        <header>
            <div id="image">
                <img src="\images\fnatic.png" height="200px" width="600px" />
            </div>
        </header>
        <main>
            
            <content>
          
                @RenderBody()
            </content>
            <aside>
                <h4>Kontakieren</h4>
                <h6>Generelle Anfrage</h6>
                <p>contact@fnatic.com</p>
                <h6>Partner</h6>
                <p>partners@fnatic.com</p>
                <h6>Support</h6>
                <p>support@fnatic.com</p>
            </aside>
        </main>
        <footer>
            <nav>
                <partial name="Navigation" />
            </nav>
        </footer>
    </div>

   


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
